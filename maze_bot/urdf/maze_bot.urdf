<robot name="maze_bot">

    <link name="base_footprint"/>
    <link name="base">
      <inertial>
        <mass value="2" />
        <origin xyz="0 0 0.1" rpy="0 0 0" />
        <inertia ixx="0.00355" iyy="0.01265" izz="0.013832" ixy="0.0" ixz="0.0" iyz="0.0" />
      </inertial>

      <visual>
        <geometry>
          <mesh filename="package://maze_bot/meshes/base_link.stl" scale='10 10 10'/>
          <!-- <mesh filename="$(find maze_bot)/meshes/base_link.stl"/> -->
          <!-- <mesh filename="file:/path_planning_ws/install/maze_bot/share/maze_bot/meshes/base_link.stl"/> -->
          <!-- <box size="0.1 0.1 0.1"/> -->
        </geometry>
        <material name="gray">
          <color rgba=".2 .2 .2 1" />
        </material>
      </visual>
  
      <collision>
        <geometry>
          <mesh filename="package://maze_bot/meshes/base_link.stl" scale='10 10 10'/>
      </geometry>
        <contact_coefficients mu="0" kp="1000.0" kd="1.0"/>
      </collision>


    </link>
    <joint name="base_joint" type="fixed">
      <parent link="base_footprint"/>
      <child link="base"/>
      <origin rpy="0 0 0" xyz="0 0 0.0"/>
      <!-- <origin rpy="0 0 0" xyz="0 0 0.0328"/> -->
    </joint>


    <link name="wheel_right">
      <inertial>
        <mass value="0.75"/>
        <inertia ixx="0.001444223" iyy="0.00280875" izz="0.00144423" ixy="0.0" ixz="0.0" iyz="0.0"/>
        <origin/>
      </inertial>
    
        <visual>
          <geometry>
            <mesh filename="package://maze_bot/meshes/wheel_r.stl" scale='10 10 10'/>
          </geometry>

          <material name="white">
            <color rgba="1 1 1 1"/>
          </material>
        </visual>
    
        <collision>
          <geometry>
            <mesh filename="package://maze_bot/meshes/wheel_r.stl" scale='10 10 10'/>
          </geometry>
          <contact_coefficients mu="0" kp="1000.0" kd="1.0"/>
        </collision>

    </link>
  
    <joint name="wheel_right_joint" type="continuous">
      <origin xyz="0.09 0.085 0.04  " rpy="0 0.0 0.0"/>
      <!-- <origin xyz="0.0091 0.0105 0.0041" rpy="0 0.0 0.0"/> -->
      <!-- <origin xyz="0.091371 0.08328 0.036801" rpy="0 0.0 0.0"/> -->
      <parent link="base"/>
      <child link="wheel_right"/>
      <axis xyz="0.0 -1.0 0"/>
    </joint>
  
    <link name="wheel_left">
      <inertial>
        <mass value="0.75"/>
        <inertia ixx="0.001444223" iyy="0.00280875" izz="0.001444223" ixy="0.0" ixz="0.0" iyz="0.0" />
        <origin/>
      </inertial>
  
      <visual>
        
        <geometry>
          <mesh filename="package://maze_bot/meshes/wheel_l.stl" scale='10 10 10'/>
        </geometry>
        
        <material name="white">
          <color rgba="1 1 1 1"/>
        </material>
      </visual>
  
      <collision>
        <geometry>
          <mesh filename="package://maze_bot/meshes/wheel_l.stl" scale='10 10 10'/>
        </geometry>
        <contact_coefficients mu="0" kp="1000.0" kd="1.0"/>
      </collision>
    </link>
  
    <joint name="wheel_left_joint" type="continuous">
      <origin xyz="0.09 -0.1 0.04" rpy="0 0.0 3.14"/>
      <!-- <origin xyz="0.0091 -0.0105 0.0041" rpy="0 0.0 3.14"/> -->
      <!-- <origin xyz="0.091371 -0.08328 0.036801" rpy="0 0.0 3.14"/> -->
      <parent link="base"/>
      <child link="wheel_left"/>
      <axis xyz="0.0 1 0"/>
  
    </joint>
  
  
    <link name="caster">

      <inertial>
        <mass value="0.75"/>
        <inertia ixx="0.0001295" iyy="0.0001295" izz="0.000199322" ixy="0.0" ixz="0.0" iyz="0.0" />
        <origin/>
      </inertial>
  
      <visual>
        <geometry>
          <mesh filename="package://maze_bot/meshes/front_caster.stl" scale='10 10 10'/>
        </geometry>
        <material name="white">
          <color rgba="1 1 1 1"/>
        </material>
      </visual>
  
      <collision>
        <geometry>
          <mesh filename="package://maze_bot/meshes/front_caster.stl" scale='10 10 10'/>
        </geometry>
        <contact_coefficients mu="0" kp="1000.0" kd="1.0"/>
      </collision>
    </link>

    <joint name="caster_joint" type="continuous">
      <origin xyz="-0.14 -0.007 -0.03" rpy="0.0 0.0 0.0"/>
      <!-- <origin xyz="-0.0138 -0.0008 -0.003" rpy="0.0 0.0 0.0"/> -->
      <!-- <origin xyz="-0.139056 -0.00699 -0.018736" rpy="0.0 0.0 0.0"/> -->
      <axis xyz="0 0 1" />
      <parent link="base"/>
      <child link="caster"/>
    </joint>  


